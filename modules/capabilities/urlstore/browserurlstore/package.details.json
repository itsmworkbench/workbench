{
  "template":    "typescript",
  "name":        "@itsmworkbench/browserurlstore",
  "description": "loads and saves itsm schema urls used with intellimaintain using an api",
  "packageJson": {
    "dependencies":    {
      "@laoban/utils":    "${properties.laobanVersion}",
      "@laoban/fileops":  "${properties.laobanVersion}"
    },
    "devDependencies": {"jest-fetch-mock": "^3.0.3"},
    "browser":         {
      "child_process": false
    }
  },
  "links":       [
    "@itsmworkbench/domain",
    "@itsmworkbench/urlstore",
    "@itsmworkbench/git"
  ],
  "guards":      {
    "compile": true,
    "publish": true,
    "test":    true
  }
}