{
  "template":    "typescript",
  "name":        "@itsmworkbench/nodeurlstore",
  "description": "loads and saves itsm schema urls used with intellimaintain using node",
  "packageJson": {
    "dependencies":    {
      "@laoban/utils":    "1.4.50",
      "@laoban/fileops":  "1.4.50",
      "node":            "^v14.4.0"
    },
    "devDependencies": {},
    "browser":         {
      "child_process": false
    }
  },
  "links":       [
    "@itsmworkbench/domain",
    "@itsmworkbench/fileloading",
    "@itsmworkbench/urlstore",
    "@itsmworkbench/git"
  ],
  "guards":      {
    "compile": true,
    "publish": true,
    "test":    true
  }
}