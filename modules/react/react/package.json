    "@itsmworkbench/domain": "0.3.11",
    "@itsmworkbench/events": "0.3.11",
    "@itsmworkbench/eventstore": "0.3.11",
    "@itsmworkbench/jsyaml": "0.3.11",
    "@itsmworkbench/react_capabilities": "0.3.11",
    "@itsmworkbench/react_conversation": "0.3.11",
