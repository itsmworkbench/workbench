{
  "name": "@itsmworkbench/react",
  "description": "A second go at the react application",
  "version": "0.3.11",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
